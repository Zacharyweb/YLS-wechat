/*color*/

.c1 {
  color: #76a6f0 !important;
}

.c2 {
  color: #888 !important;
}

.c3 {
  color: #ff906a !important;
}

.data-empty {
  line-height: 200rpx;
  height: 200rpx;
  text-align: center;
  color: #888;
}

.commend, .commend {
  padding: 20rpx 0;
}

.com-title {
  position: relative;
  padding-left: 20rpx;
}

.com-title::before {
  content: " ";
  position: absolute;
  top: 0;
  left: 0;
  width: 8rpx;
  background: #ff906a;
  height: 100%;
}

.com-num {
  background: #ff906a;
  color: #fff;
  width: 40rpx;
  height: 30rpx;
  border-radius: 15rpx;
  font-size: 9pt;
  margin-left: 20rpx;
  line-height: 30rpx;
  text-align: center;
}

.com-photos {
  padding: 20rpx;
  white-space: nowrap;
}

.com-photo {
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  margin-left: -15rpx;
}

.com-photo-first {
  margin-left: 0 !important;
}

.com-list {
  margin: 20rpx 40rpx;
  color: #666;
}

.com-list .com-list-item {
  margin: 20 rpx;
}

.com-list .com-list-item .com-photo {
  margin-left: 0;
}

.com-list .com-list-item .com-comment {
  padding: 0 0 20rpx 20rpx;
}

.com-comment .com-time {
  color: #888;
  font-size: 9pt;
  font-weight: normal;
}

.com-comment .com-content {
  font-size: 11pt;
  padding: 5rpx 0;
}

.com-comment .com-name {
  position: relative;
  font-size: 12pt;
}

.com-reply {
  position: relative;
  margin: 10rpx 0;
  padding-top: 30rpx;
}

.com-reply .com-name::before {
  content: " ";
  position: absolute;
  top: 0;
  left: -20rpx;
  width: 8rpx;
  background: #ff906a;
  height: 100%;
}

.com-comment .com-reply::after {
  content: " ";
  width: 200%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  border-top: 1px solid #d9d9d9;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
}

/*dialog*/

.dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99998;
  background: rgba(0, 0, 0, 0.7);
  display: none;
}

.dialog__container {
  position: fixed;
  top: 10%;
  left: 50rpx;
  width: 650rpx;
  background: white;
  z-index: 99999;
  display: none;
  border-radius: 10rpx;
}

.dialog--show .dialog__container {
  display: block;
}

.dialog--show .dialog__mask {
  display: block;
}

.input-comt {
  box-shadow: 0 0 10rpx #ccc;
  padding: 20rpx;
  width: calc(100% - 40rpx);
  height: 200rpx;
  border-radius: 10rpx;
}

.input-single {
  padding: 20rpx;
  width: calc(100% - 40rpx);
  height: 50rpx;
  border-radius: 10rpx;
}

/*标签切换*/

.top-bar {
  margin-left: 20rpx;
  position: relative;
}

.top-bar .icon-sousuo {
  position: absolute;
  right: 0;
  line-height: 150%;
}

.top-bar .bar-item {
  padding: 5px 30rpx 10rpx 0;
}

.bar-item-select {
  position: relative;
}

.bar-item-select::after {
  content: " ";
  position: absolute;
  bottom: -10rpx;
  left: 0;
  background: #fff;
  width: calc(100% - 30rpx);
  height: 3px;
}

/*文章列表*/

.a-group {
  font-size: 12pt;
  margin: 20rpx 0 20rpx 0;
}

.a-item {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 20rpx;
}

.a-item .a-item-img {
  width: 250rpx;
  height: 200rpx;
}

.a-item-r {
  padding: 0 20rpx;
  width: 400rpx;
  line-height: 130%;
}

.a-item-r .title {
  font-weight: bold;
  color: #353535;
}

.a-item-r .desc {
  font-size: 11pt;
  color: #888;
}

